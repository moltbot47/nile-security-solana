{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(nile_http_requests_total[5m])",
          "legendFormat": "{{method}} {{path}}"
        }
      ]
    },
    {
      "title": "Response Status Codes",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "nile_http_status_total",
          "legendFormat": "{{code}}"
        }
      ]
    },
    {
      "title": "Scans Total",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "nile_scans_total",
          "legendFormat": "Total Scans"
        }
      ]
    },
    {
      "title": "Avg Scan Duration",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 4 },
      "targets": [
        {
          "expr": "nile_scan_avg_duration_seconds",
          "legendFormat": "Avg Duration (s)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Request Duration (p50/p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "nile_http_request_duration_seconds_sum / nile_http_request_duration_seconds_count",
          "legendFormat": "avg {{method}} {{path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Error Rate (4xx + 5xx)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "rate(nile_http_status_total{code=~\"4..|5..\"}[5m])",
          "legendFormat": "{{code}}"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["nile", "solana", "security"],
  "templating": { "list": [] },
